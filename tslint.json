{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "interface-name": [
      true,
      "never-prefix"
    ],
    "quotemark": [
      true,
      "single"
    ],
    "arrow-parens": [
      true,
      "ban-single-arg-parens"
    ],
    "curly": [
      true,
      "ignore-same-line"
    ],
    "no-unused-expression": [
      true,
      "allow-fast-null-checks"
    ],
    "variable-name": {
      "options": [
        "ban-keywords",
        "check-format",
        "allow-leading-underscore"
      ]
    },
    "no-empty": [
      true,
      "allow-empty-functions"
    ]
  },
  "rulesDirectory": []
}
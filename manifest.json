{
    "manifest_version": 2,
    "name": "__MSG_extensionName__",
    "version": "0.0.10",
    "default_locale": "ja",
    "description": "__MSG_extensionDescription__",
    "icons": {
        "128": "resources/icons/128.png"
    },
    "page_action": {
        "default_title": "__MSG_extensionName__",
        "default_popup": "resources/popup.html"
    },
    "permissions": [
        "https://unipos.me/*",
        "storage",
        "activeTab",
        "declarativeContent"
    ],
    "background": {
        "scripts": [
            "dist/background.js"
        ],
        "persistent": false
    },
    "options_ui": {
        "page": "resources/options.html"
    }
}
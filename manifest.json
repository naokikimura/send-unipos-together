{
    "manifest_version": 2,
    "name": "Send Unipos together",
    "version": "0.0.9",
    "default_locale": "ja",
    "description": "This extension allows you to send Unipos to multiple destinations at once.",
    "icons": {
        "128": "resources/icons/128.png"
    },
    "page_action": {
        "default_title": "Send Unipos together",
        "default_popup": "resources/popup.html"
    },
    "permissions": [
        "https://unipos.me/*",
        "storage",
        "activeTab",
        "declarativeContent"
    ],
    "background": {
        "scripts": [
            "dist/background.js"
        ],
        "persistent": false
    },
    "options_ui": {
        "page": "resources/options.html"
    }
}